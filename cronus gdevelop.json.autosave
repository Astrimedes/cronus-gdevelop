"type":{"inverted":false,"value":"ObjectVariableAsBoolean"},"parameters":["Uranus","AllowedAttack","True"],"subInstructions":[]}],"actions":[],"events":[{"disabled":false,"folded":false,"type":"BuiltinCommonInstructions::JsCode","inlineCode":"/** @type {gdjs.SpriteRuntimeObject} */\nvar enemy = runtimeScene.getObjects(\"Uranus\")[0];\nlet enemyVars = enemy.getVariables();\n\n/** @type {gdjs.SpriteRuntimeObject[]} */\nlet objs = objects;\nobjs.sort((a,b) => enemy.getDistanceToObject(a) - enemy.getDistanceToObject(b));\n\nlet targets = objects.filter(sp => sp.getBehavior(\"Health\")._behaviorData.Health > 0);\nif (!targets.length) {\n    enemy.setAnimation(0);\n    return;\n}\n\n/** @type {gdjs.SpriteRuntimeObject} */\nlet target = targets[0];\nlet targetVars = target.getVariables();\n\n/** @type {gdjs.PathfindingRuntimeBehavior} */\nvar pathfinding = enemy.getBehavior(\"Pathfinding\");\n// if (!pathfinding.pathFound() || enemyVars.get(\"TargetName\").getAsString() != targetVars.get(\"Name\").getAsString()) {\n    let hw = enemy.getWidth()/2;\n    let targetX = target.x + (Math.sign(enemy.x - target.x) * hw); \n    let targetY = target.y;\n    pathfinding.moveTo(runtimeScene, targetX, targetY);\n    enemyVars.get(\"TargetName\").setString(targetVars.get(\"Name\").getAsString());\n// }\npathfinding.setMaxSpeed(0);\npathfinding.setSpeed(0);\n\n/** @type {gdjs.TopDownMovementRuntimeBehavior} */\nvar topDownMove = enemy.getBehavior(\"TopDownMovement\");\n\n// movement according to path\nlet x = pathfinding.getNextNodeX();\nlet y = pathfinding.getNextNodeY();\n\nlet xDist = x - enemy.x;\nlet yDist = y - enemy.y; \n\nif (xDist > 0) {\n    topDownMove.simulateRightKey();\n} else if (xDist < 0) {\n    topDownMove.simulateLeftKey();\n}\nif (yDist > 0) {\n    topDownMove.simulateDownKey();\n} else if (yDist < 0) {\n    topDownMove.simulateUpKey();\n}\n\n\n// decide to attack based on random distance\nlet attackDist = (enemy.getWidth() * 0.5) + (Math.random() * (enemy.getWidth() * 0.3));\nlet attacking = false;\nif (yDist < (enemy.getHeight() * 0.6) &&  enemy.getDistanceToObject(target) < attackDist) {\n    enemy.setVariableString(enemyVars.get(\"State\"), \"attack\");\n    enemy.setVariableBoolean(enemyVars.get(\"AllowedAttack\"), false);\n    enemy.setVariableNumber(enemyVars.get(\"AttackDelay\"), 1250 + (Math.random() * 500));\n}\n\nif (targetX - enemy.x > 0) {\n    enemy.flipX(false);\n} else {\n    enemy.flipX(true);\n}\nlet anim = attacking ? 3 : xDist || yDist ? 1 : 0;\nenemy.setAnimation(anim);\n\n\n\n\n","parameterObjects":"Targets","useStrict":true,"eventsSheetExpanded":true}]},{"disabled":true,"folded":false,"type":"BuiltinCommonInstructions::Standard","conditions":[{"type":{"inverted":true,"value":"PathfindingBehavior::PathFound"},"parameters":["Uranus","Pathfinding"],"subInstructions":[]}],"actions":[{"type":{"inverted":false,"value":"ModVarScene"},"parameters":["Temp","=","RandomInRange(0, 7)"],"subInstructions":[]}],"events":[{"disabled":false,"folded":false,"type":"BuiltinCommonInstructions::Standard","conditions":[{"type":{"inverted":false,"value":"BuiltinCommonInstructions::Or"},"parameters":[],"subInstructions":[{"type":{"inverted":false,"value":"VarScene"},"parameters":["Temp","=","0"],"subInstructions":[]},{"type":{"inverted":false,"value":"VarScene"},"parameters":["Temp","=","1"],"subInstructions":[]},{"type":{"inverted":false,"value":"VarScene"},"parameters":["Temp","=","2"],"subInstructions":[]}]}],"actions":[{"type":{"inverted":false,"value":"TopDownMovementBehavior::SimulateUpKey"},"parameters":["Uranus","TopDownMovement"],"subInstructions":[]}],"events":[]},{"disabled":false,"folded":false,"type":"BuiltinCommonInstructions::Standard","conditions":[{"type":{"inverted":false,"value":"BuiltinCommonInstructions::Or"},"parameters":[],"subInstructions":[{"type":{"inverted":false,"value":"VarScene"},"parameters":["Temp","=","5"],"subInstructions":[]},{"type":{"inverted":false,"value":"VarScene"},"parameters":["Temp","="